{
    "extends": "../../tsconfig",
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "allowUnreachableCode": false,
        "declaration": true,
        "declarationMap": false,
        "jsx": "react",
        "jsxFactory": "h",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "dist",
        "rootDir": "src",
        "target": "es2017"
    },
    "include": ["src"]
}
